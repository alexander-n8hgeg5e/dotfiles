#!/usr/bin/env python3
from pylib.screen_utils.env import parse_display_var
from subprocess import check_call,Popen

display_host=parse_display_var().split(':')[0]
cmds=[]

cmd = [ 'setup_x', display_host ]
cmds.append(cmd)                                      

cmd = ['setup_xrandr',display_host]
cmds.append(cmd)

cmd = ['rind','clipster', '-d']
cmds.append(cmd)

for cmd in cmds:
    check_call(cmd)

p=Popen(['spectrwm'])
p.wait()

set -u LD_PRELOAD
setup_x

#xserver_start &
pulseaudio -D &
setup_xrandr
setup_x
#redshift -c ~/.redshift_esadc.conf &
clipster -d &
screen_on_control &
st -e TM &
sc -r &
spectrwm
#i3 -c "$HOME""/.config/i3/config_esadc:0"
